= rounded_corner(:title => "#{@album.name}'s Photos", :id => "albums") do
  -@album.photos.each do |a|
    .album_pic_panel
      %span{:class => 'span_album'}
        = link_to(image_tag('edit.png'), edit_album_photo_path(@album,a))
        = link_to(image_tag('delete.png'), album_photo_path(@album,a), :method =>:delete)
      .album_img_wrap
        = image_tag(a.image.url('original'), :width => 160, :height => 115)

