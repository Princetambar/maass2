.fields
  = f.inputs do
    = f.input :education_from_year, :label => "From Year", :as => :select, :collection => User::EDU_YEAR, :prompt => true
    = f.input :education_to_year, :label => "To Year", :as => :select, :collection => User::EDU_YEAR, :prompt => true
    = f.input :institution
  = link_to_remove_fields "remove", f