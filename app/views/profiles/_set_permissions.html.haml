= semantic_form_for(@profile) do |form|
  - rounded_med_corner(:id => "set_permissions", :title => "Permissions") do
    %table
      %tr
        %th
        %th Just Me
        %th My Friends
        %th Everyone
      = raw form.render_associated_form(current_user.profile.profile_permissions)
    = form.commit_button "Update Permissions"