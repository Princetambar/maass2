= form_for(@title,:as=>:title,:url => update_title_admin_preferences_path,:html =>{:method => :put}) do |f|
  - @titles.each_with_index do|title, index|
    = fields_for "titles[#{index}]",title do |f|
      = hidden_field_tag "titles[#{index}][id]" ,title.id
      .row_complete
        = f.text_field :name, :title => "Title",:class => "text_field"
  %button.buttons{:type => "submit"}
    = image_tag('Update.png')