.widget_complete
  %span.widget_complete_top
  %h2.widget_compelete_title Edit Event
  - form_for @event, :url => admin_event_path(@event), :html => {:method=> :put} do |form|
    = render :partial => 'form' , :locals =>{:f => form}
    %div
      %button.buttons.gmap{:type => "submit"}
        = image_tag("Update.png")
  %span.widget_complete_btm
