    = csrf_meta_tag
  %body
  .main_container
    .header_container
      = link_to image_tag("b.gif", :height => 120, :width => 345, :class => "header_back", :alt => SITE_FULL_NAME), homes_path
    .content_container
      = render 'layouts/header'
      %br
      %br
      = flash[:notice]
      %br
      = yield
      %br